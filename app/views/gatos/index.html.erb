<h1>Listing gatos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Comentario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gatos.each do |gato| %>
  <tr>
    <td><%= gato.nombre %></td>
    <td><%= gato.comentario %></td>
    <td><%= link_to 'Show', gato %></td>
    <td><%= link_to 'Edit', edit_gato_path(gato) %></td>
    <td><%= link_to 'Destroy', gato, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Gato', new_gato_path %>
